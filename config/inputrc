set bell-style none
set enable-bracketed-paste off

"\e[1~": beginning-of-line
"\e[4~": end-of-line
"\e[5~": history-search-backward
"\e[6~": history-search-forward

"\e[1;5D": backward-word
"\e[1;5C": forward-word
"\e[1;2D": backward-char
"\e[1;2C": forward-char
"\e[1;6D": backward-char
"\e[1;6C": forward-char

"\eOc":forward-word
"\eOd":backward-word

"\e[13;2u":accept-line
"\e[13;5u":accept-line
